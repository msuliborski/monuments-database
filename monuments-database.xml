        <el:monument monument-id="cattedrale-di-santa-maria-del-fiore" city-id="florence" type-id="church-cathedral">
            <el:monument-name>Cattedrale di Santa Maria del Fiore</el:monument-name>
            <el:monument-author>Arnolfo di Cambio</el:monument-author>
            <el:completion-date>
                <el:completion-day>09</el:completion-day>
                <el:completion-month>09</el:completion-month>
